{
  "name": "signalfx-defaults",
  "signatures": [
    {
      "config": "activemq.default.conf.tmpl",
      "type": "activemq",
      "enabled": true,
      "labels": ["app"],
      "rules": [
        {
          "comparator": "contains",
          "path": "ContainerImage",
          "value": "activemq"
        },
        {
          "comparator": "eq",
          "path": "NetworkPrivatePort",
          "value": 1099
        },
        {
          "comparator": "nexists",
          "path": "ContainerLabel-signalfx-integration",
          "value": ""
        }
      ]
    },
