LoadPlugin python
<Plugin "python">
  ModulePath "{{Globals.PluginRoot}}/mongodb-collectd-plugin"
  Import "mongodb"
  <Module "{{.Name}}"> 
    Host "{{.Host}}"
    Port "{{.Port}}"
    User "{{Secret "SIGNALFX_MONITOR_USER"}}"
    Password "{{Secret "SIGNALFX_MONITOR_PASSWORD"}}"
    Database "admin"
  </Module>
</Plugin>
