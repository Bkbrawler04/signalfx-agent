<LoadPlugin "apache">
  Interval {{.IntervalSeconds}}
</LoadPlugin>
<Plugin "apache">
  {{range .Endpoints}}
  <Instance "{{withDefault .name .id}}[{{encodeDimsForPluginInstance .dimensions}}]">
    URL "http://{{.host}}:{{.port}}/mod_status?auto"
    {{with .username -}}
    User "{{.}}"
    {{end}}
    {{with .password}}
    Password "{{.}}"
    {{end}}
  </Instance>
  {{end}}
</Plugin>
