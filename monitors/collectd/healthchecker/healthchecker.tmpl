LoadPlugin "python"
<Plugin python>
  ModulePath "{{Globals.PluginRoot}}/health_checker"

  Import "health_checker"
{{range .Endpoints}}
  <Module health_checker>
    URL "{{renderValue $.C.URL .}}"
    Instance "{{withDefault .Name .ID}}[{{encodeDimsForPluginInstance $.C.ExtraDimensions .Dimensions}}]"
    JSONKey "{{$.C.JSONKey}}"
    JSONVal "{{$.C.JSONVal}}"
  </Module>
{{end}}
</Plugin>
