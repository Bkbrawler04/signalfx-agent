{
  "name": "signalfx-defaults",
  "signatures": [
    {
      "config": "apache.default.conf.tmpl",
      "type": "apache",
      "enabled": true,
      "labels": ["app"],
      "rules": [
        {
          "comparator": "contains",
          "path": "ContainerImage",
          "value": "apache"
        }
      ]
    },
    {
      "config": "redis.default.conf.tmpl",
      "type": "redis",
      "enabled": true,
      "labels": ["app"],
      "rules": [
        {
          "comparator": "eq",
          "path": "ContainerLabel-signalfx-integration",
          "value": "redis"
        }
      ]
    }
  ]
}
